<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:windowSoftInputMode="adjustPan"
    tools:context=".RecommendMainActivity">

    <ImageView
        android:id="@+id/bar"
        android:layout_width="417dp"
        android:layout_height="90dp"
        android:padding="0sp"
        android:scaleType="fitCenter"
        android:src="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0095" />

    <ImageButton
        android:id="@+id/home_btn"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginLeft="5dp"
        android:background="@color/tumyeongsaeg"
        android:padding="0sp"
        android:scaleType="fitCenter"
        android:onClick="onClick"
        android:src="@drawable/home_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.988" />

    <ImageButton
        android:id="@+id/recommend_btn"
        android:layout_width="52dp"
        android:layout_height="54dp"
        android:layout_marginLeft="5dp"
        android:background="@color/tumyeongsaeg"
        android:padding="0sp"
        android:onClick="onClick"
        android:scaleType="fitCenter"
        android:src="@drawable/recommend_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.250"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.988" />

    <ImageButton
        android:id="@+id/smellnote_btn"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginLeft="5dp"
        android:onClick="onClick"
        android:background="@color/tumyeongsaeg"
        android:padding="0sp"
        android:scaleType="fitCenter"
        android:src="@drawable/smellnote_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.988" />

    <ImageButton
        android:id="@+id/camera_search"
        android:layout_width="46dp"
        android:layout_height="59dp"
        android:layout_marginLeft="5dp"
        android:layout_weight="1"
        android:onClick="onClick"
        android:background="@color/tumyeongsaeg"
        android:padding="0sp"
        android:scaleType="fitCenter"
        android:src="@drawable/camera_search_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.708"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9901" />

    <ImageButton
        android:id="@+id/mypage_btn"
        android:layout_width="63dp"
        android:layout_height="76dp"
        android:layout_marginLeft="5dp"
        android:onClick="onClick"
        android:background="@color/tumyeongsaeg"
        android:padding="0sp"
        android:scaleType="fitCenter"
        android:src="@drawable/mypage_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.959"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />



</androidx.constraintlayout.widget.ConstraintLayout>